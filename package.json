{
  "name": "react-graphql-app",
  "version": "1.0.0",
  "description": "React GraphQL Application",
  "main": "server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:server": "cross-env NODE_ENV=development nodemon --exec babel-node server",
    "start:client": "cross-env NODE_ENV=development webpack-dev-server --mode=development --config config/webpack/webpack.dev.js",
    "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start": "cross-env NODE_ENV=production node dist/server",
    "build:client": "cross-env NODE_ENV=production webpack --mode=production  --config config/webpack/webpack.prod.js --color --progress",
    "build:server": "cross-env NODE_ENV=production babel -d ./dist/server server",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "lint": "eslint '**/*.{js,jsx}' --ignore-path .eslintignore",
    "lint:fix": "eslint '**/*.{js,jsx}' --fix --ignore-path .eslintignore",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamedsamara/react-graphql-app.git"
  },
  "keywords": [
    "node",
    "express",
    "mongoose"
  ],
  "author": "Mohamed Samara (https://github.com/mohamedsamara)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohamedsamara/react-graphql-app/issues"
  },
  "homepage": "https://github.com/mohamedsamara/react-graphql-app#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-merge": "^4.2.1",
    "webpack-pwa-manifest": "^4.1.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
